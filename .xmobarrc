Config { font = "-*-Fixed-Bold-R-Normal-*-13-*-*-*-*-*-*-*"        , borderColor = "#3B8686"        , border = TopB        , bgColor = "#3B8686"        , fgColor = "white"        , position = TopW L 100        , commands = [ Run Weather "CYVR" ["-t","<tempC>C","-L","19","-H","-101","--normal","green","--high","red","--low","lightblue"] 36000                        , Run Network "eth0" ["-L","0","-H","32","--normal","green","--high","red"] 10                        , Run Network "wlan0" ["-L","0","-H","32","--normal","green","--high","red"] 10                        , Run Cpu ["-L","3","-H","50","--normal","green","--high","red"] 10                        , Run Memory ["-t","Mem: <usedratio>%"] 10                        , Run Swap [] 10                        , Run Date "%a %b %_d %Y %H:%M:%S" "date" 10                        , Run StdinReader                        ]        , sepChar = "%"        , alignSep = "}{"        , template = "%StdinReader% | %cpu% | %memory% * %swap% | %eth0% - %wlan0% }{%date% |  %CYVR% "        }